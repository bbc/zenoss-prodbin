{
    "servicePath": "/=ZenPacks.zenoss.Example",
    "serviceDefinition": {
        "Name": "webservice",
        "Command": "/usr/bin/python -m SimpleHTTPServer 8000",
        "Endpoints": [{
                "Name": "websvc",
                "Application": "websvc",
                "PortNumber": 8000,
                "Protocol": "tcp",
                "Purpose": "export"
            }
        ],
        "ImageId": "centos:6.4",
        "Instances": {
            "Min": 1
        },
        "Launch": "auto",
        "Tags": [
            "daemon"
        ]
    }
}